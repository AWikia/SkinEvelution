<div class="evelution-floating-actions page-side-tools__wrapper">
	<div class="evelution-floating-actions-container cpe-floating-button-group is-vertical page-side-tools">
		{{^has-forced-full-width}}
			<a class="cpe-floating-button accent toggle-size page-side-tool content-size-toggle" onclick="ToggleWidth()" cpe-tooltip="{{msg-evelution-togglesize}}">
				<span class="cpe-icon material-icons wide-off">
					open_in_full
				</span>
				<span class="cpe-icon material-icons wide-on">
					close_fullscreen
				</span>
			</a>
		{{/has-forced-full-width}}
		<a class="cpe-floating-button accent accent-toggle page-side-tool" onclick="ToggleAccent()" cpe-tooltip="{{msg-evelution-toggleaccent}}">
			<span class="cpe-icon material-icons">
				<span class="accented-off">
					invert_colors
				</span>
				<span class="accented-on">
					invert_colors_off
				</span>
			</span>
		</a>
		<!-- TableOfContents -->
		<div class="cpe-dropdown ftoc td-off" cpe-orient="land">				
			<div class="cpe-floating-button accent ftoc page-side-tool" cpe-tooltip="{{msg-toc}}">
				<span class="cpe-icon material-icons">
					description
				</span>
			</div>
			<div class="cpe-dropdown__content ftoc cpe-visual-appearance">					
				<div class="toc floating_toc" style="display:contents; border:0; background:transparent; display:block;">
					<ul class="cpe-list toc floating_toc" id="toc" style="border:0; background:transparent; display:block;" role="navigation">
					</ul>
				</div>
			</div>
		</div>
		<!-- Contrast Modes -->
		<div class="cpe-dropdown contrast-modes" cpe-orient="land">				
			<div class="cpe-floating-button accent contrast-modes page-side-tool" cpe-tooltip="{{msg-evelution-contrast-modes}}">
				<span class="cpe-icon material-icons">
					contrast
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-visual-appearance">					
				 <ul class="cpe-list is-linked cpe-contrast-modes has-icons">						
					<li data-contrast-mode="auto" title="Auto">
						<a onclick="contrastmode('auto')">
							<span class="cpe-icon material-icons">brightness_auto</span>
						</a>
					</li>
					<li data-contrast-mode="low" title="Low">
						<a onclick="contrastmode('low')">
							<span class="cpe-icon material-icons">brightness_1</span>
						</a>
					</li>
					<li data-contrast-mode="med-low" title="Medium-Low">
						<a onclick="contrastmode('med-low')">
							<span class="cpe-icon material-icons">brightness_2</span>
						</a>
					</li>
					<li data-contrast-mode="med" title="Medium">
						<a onclick="contrastmode('med')">
							<span class="cpe-icon material-icons">brightness_3</span>
						</a>
					</li>
					<li data-contrast-mode="med-hi" title="Medium-High">
						<a onclick="contrastmode('med-hi')">
							<span class="cpe-icon material-icons">brightness_4</span>
						</a>
					</li>
					<li data-contrast-mode="hi" title="High">
						<a onclick="contrastmode('hi')">
							<span class="cpe-icon material-icons">brightness_5</span>
						</a>
					</li>
					<li data-contrast-mode="hi-vhi" title="High-Very High">
						<a onclick="contrastmode('hi-vhi')">
							<span class="cpe-icon material-icons">brightness_6</span>
						</a>
					</li>
					<li data-contrast-mode="vhi" title="Very High">
						<a onclick="contrastmode('vhi')">
							<span class="cpe-icon material-icons">brightness_7</span>
						</a>
					</li>
				</ul>
			</div>
		</div>
	{{^has-no-color-management}}
		<!-- Color Modes -->
		<div class="cpe-dropdown color-modes color-management-on forced-colors-off" cpe-orient="land">				
			<div class="cpe-floating-button accent color-modes page-side-tool" cpe-tooltip="{{msg-evelution-colormodes}}">
				<span class="cpe-icon material-icons">
					hdr_auto
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-visual-appearance" style="top:-100%;">					
				 <ul class="cpe-list is-linked cpe-color-modes has-icons">						
					<li data-color-mode="auto" title="Light (Duo)">
						<a onclick="colortheme('auto')">
							<span class="cpe-icon material-icons">brightness_auto</span>
						</a>
					</li>
					<li data-color-mode="auto-r" title="Dark (Duo)">
						<a onclick="colortheme('auto-r')">
							<span class="cpe-icon material-icons">hdr_auto</span>
						</a>
					</li>
					<li data-color-mode="light" title="Light (Solo)">
						<a onclick="colortheme('light')">
							<span class="cpe-icon material-icons">light_mode</span>
						</a>
					</li>
					<li data-color-mode="dark" title="Dark (Solo)">
						<a onclick="colortheme('dark')">
							<span class="cpe-icon material-icons">dark_mode</span>
						</a>
					</li>
					<li data-color-mode="gray" title="White (Duo)">
						<a onclick="colortheme('gray')">
							<span class="cpe-icon material-icons">filter_b_and_w</span>
						</a>
					</li>
					<li data-color-mode="gray-r" title="Black (Duo)">
						<a onclick="colortheme('gray-r')">
							<span class="cpe-icon material-icons" style="--icon-scale:-1; transform:scaleX(var(--icon-scale)) rotate(var(--icon-rotate));">filter_b_and_w</span>
						</a>
					</li>
					<li data-color-mode="white" title="White (Solo)">
						<a onclick="colortheme('white')">
							<span class="cpe-icon material-icons">check_box_outline_blank</span>
						</a>
					</li>
					<li data-color-mode="black" title="Black (Solo)">
						<a onclick="colortheme('black')">
							<span class="cpe-icon material-icons">square</span>
						</a>
					</li>
					<li data-color-mode="reversed-r" title="Light (Reversi)">
						<a onclick="colortheme('reversed-r')">
							<span class="cpe-icon material-icons">invert_colors_off</span>
						</a>
					</li>
					<li data-color-mode="reversed" title="Dark (Reversi)">
						<a onclick="colortheme('reversed')">
							<span class="cpe-icon material-icons">invert_colors</span>
						</a>
					</li>

					<li data-color-mode="blu-wht" title="Light (Tint)">
						<a onclick="colortheme('blu-wht')">
							<span class="cpe-icon material-icons" style="--icon-rotate:180deg;">gradient</span>
						</a>
					</li>
					<li data-color-mode="yel-blk" title="Dark (Tint)">
						<a onclick="colortheme('yel-blk')">
							<span class="cpe-icon material-icons">gradient</span>
						</a>
					</li>
					<li data-color-mode="temp" title="Light (Temperature)">
						<a onclick="colortheme('temp')">
							<span class="cpe-icon material-icons">thermostat</span>
						</a>
					</li>
					<li data-color-mode="temp-r" title="Dark (Temperature)">
						<a onclick="colortheme('temp-r')">
							<span class="cpe-icon material-icons" style="--icon-rotate:180deg;">thermostat</span>
						</a>
					</li>
					<li data-color-mode="rtemp" title="Light (Reversi Temperature)">
						<a onclick="colortheme('rtemp')">
							<span class="cpe-icon material-icons">ac_unit</span>
						</a>
					</li>
					<li data-color-mode="rtemp-r" title="Dark (Reversi Temperature)">
						<a onclick="colortheme('rtemp-r')">
							<span class="cpe-icon material-icons">storm</span>
						</a>
					</li>

				</ul>
			</div>
		</div>
	{{/has-no-color-management}}
		<!-- Styles -->
		<div class="cpe-dropdown styles" cpe-orient="land">				
			<div class="cpe-floating-button accent styles page-side-tool" cpe-tooltip="{{msg-evelution-visual-styles}}">
				<span class="cpe-icon material-icons">
					format_paint
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-visual-appearance">					
				 <ul class="cpe-list is-linked cpe-visual-styles has-icons">						
					<li data-visual-style="standard" title="Standard">
						<a onclick="VisualStyle('standard')">
							<span class="cpe-icon material-icons">build</span>
						</a>
					</li>

				</ul>
			</div>
		</div>
	{{^has-no-color-management}}
		<!-- Colors -->
		<div class="cpe-dropdown colors forced-colors-off" cpe-orient="land">				
			<div class="cpe-floating-button accent colors page-side-tool" cpe-tooltip="{{msg-evelution-visual-colors}}">
				<span class="cpe-icon material-icons">
					palette
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-visual-appearance" style="top:-75%;">					
				 <ul class="cpe-list is-linked cpe-visual-colors has-icons">						
					<li data-visual-color="standard" title="Standard">
						<a onclick="VisualColor('standard')">
							<span class="cpe-icon material-icons">build</span>
						</a>
					</li>
					<li data-visual-color="nocolormanagement" title="No Color Management">
						<a onclick="VisualColor('nocolormanagement')">
							<span class="cpe-icon material-icons">invert_colors_off</span>
						</a>
					</li>
				</ul>
			</div>
		</div>
		<!-- Themes -->
		<div class="cpe-dropdown themes color-management-on forced-colors-off" cpe-orient="land">
			<div class="cpe-floating-button accent themes page-side-tool" cpe-tooltip="{{msg-evelution-themes}}">
				<span class="cpe-icon material-icons">
					brush
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-visual-appearance">
				<ul class="cpe-list is-linked cpe-themes has-icons">
					<li data-theme-change="theme-A">
						<a class="ThemeA" onclick="ToggleTheme('A')">
							A
						</a>
					</li>
					<li data-theme-change="theme-B">
						<a class="ThemeB" onclick="ToggleTheme('B')">
							B
						</a>
					</li>
					<li data-theme-change="theme-C">
						<a class="ThemeC" onclick="ToggleTheme('C')">
							C
						</a>
					</li>
					<li data-theme-change="theme-D">
						<a class="ThemeD" onclick="ToggleTheme('D')">
							D
						</a>
					</li>
					<li data-theme-change="theme-E">
						<a class="ThemeE" onclick="ToggleTheme('E')">
							E
						</a>
					</li>
					<li data-theme-change="theme-F">
						<a class="ThemeF" onclick="ToggleTheme('F')">
							F
						</a>
					</li>
					<li data-theme-change="theme-G">
						<a class="ThemeG" onclick="ToggleTheme('G')">
							G
						</a>
					</li>
					<li data-theme-change="theme-H">
						<a class="ThemeH" onclick="ToggleTheme('H')">
							H
						</a>
					</li>

				</ul>
			</div>
		</div>
	{{/has-no-color-management}}
	</div>
</div>