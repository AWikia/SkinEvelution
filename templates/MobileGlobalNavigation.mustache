  <header class="mpisto-global-sidebar mpisto-mobile-global-navigation">
	<div class="upper-links">
		<div class="cpe-dropdown cpe-has-dark-shadow meta-menu">
			<div class="link cpe-dropdown__toggle" cpe-tooltip="Meta">
				<span class="cpe-icon material-icons-outlined">
					language
				</span>
			</div>
		    <div class="cpe-dropdown__content cpe-is-left-aligned">
				<ul class="cpe-list cpe-is-linked">
					<li class="company">
						<a href="https://awikia.github.io/SkinEvelution">{{msg-eve-documentation}}</a>
					</li>
					<li class="github">
						<a href="https://github.com/AWikia/SkinEvelution">GitHub</a>
					</li>
					<li class="mediawiki">
						<a href="https://mediawiki.org">MediaWiki</a>
					</li>
					<li class="discord">
						<a href="https://discord.gg/a6FbV6zWFs">{{msg-eve-support}}</a>
					</li>
				</ul>
			</div>
		</div>
		<div class="cpe-dropdown cpe-has-dark-shadow explore-menu">
			<div class="link cpe-dropdown__toggle" cpe-tooltip={{msg-eve-explore}}>
				<span class="cpe-icon material-icons-outlined">
					arrow_forward_ios
				</span>
			</div>
		    <div class="cpe-dropdown__content cpe-is-left-aligned">
		        {{#data-footer}}
					{{!}}{{#data-places}}{{>FooterList}}{{/data-places}}
				{{/data-footer}}
			</div>
		</div>
	</div>
	<div class="bottom-links">
		<!-- Language Variants -->
		<div class="cpe-dropdown cpe-has-dark-shadow variants-menu">
			<div class="link cell cpe-dropdown__toggle" cpe-tooltip="{{msg-eve-variants}}">
				<span class="cpe-icon material-icons-outlined">
					font_download
				</span>
			</div>
			<div class="cpe-dropdown__content">
				{{#data-portlets.data-variants}}{{>Portlet2}}{{/data-portlets.data-variants}}
			</div>
		</div>
		<!-- In other Langauges -->
		<div class="cpe-dropdown cpe-has-dark-shadow language-menu">
			<div class="link cell cpe-dropdown__toggle" cpe-tooltip="{{msg-otherlanguages}}">
				<span class="cpe-icon material-icons-outlined">
					translate
				</span>
			</div>
			<div class="cpe-dropdown__content cpe-is-right-aligned">
				{{#data-portlets.data-languages}}{{>Portlet2}}{{/data-portlets.data-languages}}
			</div>
		</div>

		<!-- Search -->
		<div class="cpe-dropdown cpe-has-dark-shadow search">
			<div class="link cpe-dropdown__toggle" cpe-tooltip={{msg-search}}>
				<span class="cpe-icon material-icons-outlined">
					search
				</span>
			</div>
		    <div class="cpe-dropdown__content cpe-is-right-aligned">
				<ul class="cpe-list">
					<li>

					{{#data-search-box}}
					<form action="{{form-action}}" role="search" class="mw-portlet" id="p-search">
						<div class="pageheader-search">
							<input type="search" name="search" class="cpe-input cpe-is-borderline" placeholder="{{msg-searchsuggest-search}}" autocapitalize="sentences" accesskey="f" id="searchInput" autocomplete="off">
							<input style="width:0px; height:0px; visibility:hidden; position:absolute;" type="submit" id="searchButton2M"></input>
							<label for="searchButton2M" class="cpe-button cpe-is-borderline">
								<span class="cpe-icon cpe-icon-small material-icons-outlined">
									search
								</span>
							</label>
						</div>
					</form>
					{{/data-search-box}}
					</li>
				</ul>
			</div>
		</div>
	  <!-- Notifications -->
		<a class="link cell cpe-dropdown__toggle" cpe-tooltip="{{msg-notifications}}" href="{{html-notices-link}}">
			<span class="cpe-icon material-icons-outlined">
				notifications
			</span>
		</a>
      <!-- Themes -->
		  <div class="cpe-dropdown cpe-has-dark-shadow themes color-management-on" >
			<div class="link cpe-dropdown__toggle" cpe-tooltip={{msg-eve-themes}}>
								<span class="cpe-icon material-icons-outlined">
									palette
								</span>
			</div>
			<div class="cpe-dropdown__content">
				<ul class="cpe-list cpe-is-linked cpe-themes">
					<li data-theme-change="theme-A">
						<a class="ThemeA">
							{{msg-eve-theme}} A
						</a>
					</li>
					<li data-theme-change="theme-B">
						<a class="ThemeB">
							{{msg-eve-theme}} B
						</a>
					</li>
					<li data-theme-change="theme-C">
						<a class="ThemeC">
							{{msg-eve-theme}} C
						</a>
					</li>
					<li data-theme-change="theme-D">
						<a class="ThemeD">
							{{msg-eve-theme}} D
						</a>
					</li>
				</ul>
			</div>
		</div>

		<div class="cpe-dropdown cpe-has-dark-shadow user-profile">
			<div class="link cell cpe-dropdown__toggle" cpe-tooltip="{{msg-personaltools}}">
								<span class="cpe-icon material-icons-outlined">
									account_circle
								</span>
			</div>
			<div class="cpe-dropdown__content cpe-is-right-aligned">
				{{#data-portlets.data-user-menu}}{{>Portlet2}}{{/data-portlets.data-user-menu}}
			</div>
		</div>
	</div>
  </header>
<!-- End Global Nav -->
