	<header class="evelution-minibar is-fixed-positioned fandom-sticky-header">
		<div class="cpe-dropdown local-navigation">
			<a class="link cell cpe-dropdown__toggle" cpe-tooltip-position="bottom" cpe-tooltip="{{msg-navigation}}">
				<span class="cpe-icon cpe-dropdown__toggle-chevron material-icons focused-dropdown-off">
					menu
				</span>
				<span class="cpe-icon cpe-dropdown__toggle-chevron material-icons focused-dropdown-on">
					menu_open
				</span>
			</a>
			<div class="cpe-dropdown__content is-left-aligned">
				<ul class="cpe-list">
						<section class="evelution-module toc-module rail-module td-off">
							<h2 class="rail-module__header">
								<span class="cpe-icon material-icons">
								description
								</span>
								<span class="module-title">
								{{msg-toc}}
								</span>
							</h2>
							<div class="evelution-module-content">
								<div class="toc floating_toc" style="display:contents; border:0; background:transparent; display:block; color:inherit;">
									<ul class="toc module-toc floating_toc" id="toc" style="border:0; background:transparent; display:block; color:inherit;" role="navigation">
									</ul>
								</div>
							</div>
						</section>
							{{#data-portlets-sidebar}}
							{{#data-portlets-first}}{{>PortletSLN2}}{{/data-portlets-first}}
							{{#array-portlets-rest}}{{>PortletSLN2}}{{/array-portlets-rest}}
							{{#data-portlets.data-languages}}{{>PortletSLN2}}{{/data-portlets.data-languages}}
							{{#data-portlets.data-variants}}{{>PortletSLN2}}{{/data-portlets.data-variants}}
							{{/data-portlets-sidebar}}
				</ul>
			</div>
		</div>
		<div class="title-container">
			<div class="logo-with-wordmark">
				{{#data-logos}}
					{{^evelution-nostickylogo}}
							{{#data-logos}}
								{{#icon}}
									{{#has-mono-logo}}
									<a class="wordmark" href="{{link-mainpage}}">							
										<div class="icon-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}})">
											<img src="{{.}}" style="opacity:0;">
										</div>
									</a>
									{{/has-mono-logo}}
									{{^has-mono-logo}}
									<a class="wordmark invertion-on" href="{{link-mainpage}}">							
										<img src="{{.}}">
									</a>
									{{/has-mono-logo}}
								{{/icon}}
								{{^icon}}
									{{#1x}}
										{{#has-mono-logo}}
										<a class="wordmark" href="{{link-mainpage}}">							
											<div class="icon-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}})">
												<img src="{{.}}" style="opacity:0;">
											</div>
										</a>
										{{/has-mono-logo}}
										{{^has-mono-logo}}
										<a class="wordmark invertion-on" href="{{link-mainpage}}">							
											<img src="{{.}}">
										</a>
										{{/has-mono-logo}}
									{{/1x}}
								{{/icon}}
							{{/data-logos}}
					{{/evelution-nostickylogo}}
				{{/data-logos}}
				<h1 class="title cell" style="text-align:left" translate="no">
					{{#data-logos}}
						{{#wordmark}}
							<div style="--height:{{height}}px; display:contents">
								{{#src}}
								<a href="{{link-mainpage}}" class="title-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}});"><span style="visibility:hidden;">Abc</span></a>
								{{/src}}
								{{^src}}
								<a href="{{link-mainpage}}" class="title-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}});"><span style="visibility:hidden;">Abc</span></a>
								{{/src}}
							</div>
						{{/wordmark}}
						{{^wordmark}}<a href="{{link-mainpage}}">{{msg-sitetitle}}</a>{{/wordmark}}
					{{/data-logos}}
				</h1>
			</div>
			<div class="namespace-with-title">
				<h1 class="title cell" style="text-align:left" translate="no">
					<a class="has-icon" href="{{link-mainpage}}">{{html-title}}</a>
				</h1>
			</div>
		</div>
		<div class="counters cell">
			<div class="counter articles"><big>{{html-articlecount}}</big> <span class="cpe-icon material-icons">article</span>  <span class="text">{{msg-evelution-pages}}</span></div>
			<div class="counter photos"><big>{{html-filecount}}</big> <span class="cpe-icon material-icons">photo</span> <span class="text">{{msg-evelution-files}}</span></div>
		</div>
		<div class="counters cell">
			<div class="counter edits"><big>{{html-editcount}}</big> <span class="cpe-icon material-icons">edit</span> <span class="text">{{msg-evelution-edits}}</span></div>
			<div class="counter users"><big>{{html-usercount}}</big> <span class="cpe-icon material-icons">person</span> <span class="text">{{msg-evelution-users}}</span></div>
		</div>
		<div class="desktop-links">
			{{>CommunityLinks}}
		</div>
		<div class="mobile-links">
			{{>MobileCommunityLinks}}
		</div>
	</header>
