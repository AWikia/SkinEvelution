		<div class="mpisto-header-container">
			<section class="mpisto-header-container-wrapper">
				<a class="wordmark" href="{{link-mainpage}}">
				{{#data-logos}}
					{{#icon}}<img src="{{.}}">{{/icon}}
					{{#wordmark}}<img src="{{src}}">{{/wordmark}}
				{{/data-logos}}
				</a>



				<header class="color-header">
						<div class="table">
							<div class="tr">
								<h1 class="title cell" style="text-align:left" translate="no"><a href="{{link-mainpage}}">{{msg-sitetitle}}</a></h1>
								<div class="counters cell">
									<div class="counter articles"><big>{{html-articlecount}}</big> <span>{{msg-eve-pages}}</span></div>
									<div class="counter photos"><big>{{html-filecount}}</big> <span>{{msg-eve-files}}</span></div>
								</div>
								<a class="link cell" title="{{msg-recentchanges}}"  href="{{html-rc-link}}">
									<span class="cpe-icon material-icons-outlined">
									trending_up
									</span>
								</a>
								<a class="link cell dark-mode darkmode-off" title="{{msg-eve-darkmode}}">
									<span class="cpe-icon material-icons-outlined">
									brightness_2
									</span>
								</a>
								<a class="link cell dark-mode darkmode-on" title="{{msg-eve-lightmode}}">
									<span class="cpe-icon material-icons-outlined">
									light_mode
									</span>
								</a>
								<a class="link cell" title="{{msg-eve-files}}"  href="{{html-files-link}}">
									<span class="cpe-icon material-icons-outlined">
									photo_library
									</span>
								</a>
								<a class="link cell" title="CSS"  href="{{html-css-link}}">
									<span class="cpe-icon material-icons-outlined">
									palette
									</span>
								</a>
								<a class="link cell" title="JS"  href="{{html-js-link}}">
									<span class="cpe-icon material-icons-outlined">
									code
									</span>
								</a>
								<a class="link cell" title="{{msg-specialpages}}"  href="{{html-spages-link}}">
									<span class="cpe-icon material-icons-outlined">
									settings
									</span>
								</a>
							</div>
						</div>
				</header>

				<nav class="color-nav">
					<div class="table">
						<div class="tr">
							{{#data-portlets-sidebar}}
							{{#data-portlets-first}}{{>PortletLN}}{{/data-portlets-first}}
							{{#array-portlets-rest}}{{>PortletLN}}{{/array-portlets-rest}}
							{{/data-portlets-sidebar}}
						</div>
					</div>
			</section>
		</div>