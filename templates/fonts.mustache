@import url('https://fonts.googleapis.com/css?family=Noto+Sans:400,700|Noto+Sans+SC:100,300,400,500,700,900|Noto+Sans+TC:100,300,400,500,700,900|Noto+Sans+HK:100,300,400,500,700,900|Hind+Siliguri:300,400,500,600,700|Didact+Gothic|Kantumruy:300,400,700|Manjari:100,400,700|Padauk:400,700|Alef:400,700');

/** Secondary Fonts **/
/* 
**
M PLUS Rounded 1c = Japanese
Jua (Formerly Nanum Gothic) = Korean
Mitr = Thailandish
Mada = Arabic
NTR = Telegu
Pavanam = Tamil
Mukta = Devanagari
Noto Sans = Chinese
Baloo Da 2 = Bengali
Baloo Tamma 2 = Kannada
Battambang = Khmer
Baloo Chettan 2 = Malayalam
No font (Padauk) = Myanmar
Baloo Bhaina 2 = Oriya
Jura = Greek and Cyrillic (Old)
Arimo = Greek
Rubik = Latin, Cyrrilic and Hebrew
Raleway = Latin (Old)
**
*/
@import url('https://fonts.googleapis.com/css?family=Jura:300,400,500,600,700|M+PLUS+Rounded+1c:100,300,400,500,700,800|Mitr:200,300,400,500,600,700|Nanum+Gothic:400,700,800|Raleway:100,200,300,400,500,600,700,800,900&subset=cyrillic,cyrillic-ext,greek,greek-ext,hebrew,japanese,korean,latin-ext,thai,vietnamese');

@import url('https://fonts.googleapis.com/css?family=Jua|Mada:200,300,400,500,600,700,900|NTR|Pavanam&subset=arabic,korean,latin-ext,tamil,telugu');


@import url('https://fonts.googleapis.com/css?family=Mukta:200,300,400,500,600,700,800|Baloo+Da+2:400,500,600,700,800|Baloo+Tamma+2:400,500,600,700,800|Battambang|Baloo+Chettan 2:400,500,600,700,800|Baloo+Bhaina+2:400k,500,600,700,800');
