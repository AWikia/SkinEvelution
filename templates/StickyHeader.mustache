	<header class="mpisto-sticky-header-container full-nav">
		<a class="wordmark" href="{{link-mainpage}}">
			{{#data-logos}}
				{{#icon}}<img src="{{.}}">{{/icon}}
				{{^icon}}{{#1x}}<img src="{{.}}">{{/1x}}{{/icon}}
			{{/data-logos}}
		</a>
		<h1 class="title cell" style="text-align:left" translate="no">
			{{#data-logos}}
				{{#wordmark}}
					<div style="--height:{{height}}px; display:contents">
						{{#src}}
						<a title="{{msg-sitetitle}}" href="{{link-mainpage}}" class="title-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}});"><span style="visibility:hidden;">Abc</span></a>
						{{/src}}
						{{^src}}
						<a title="{{msg-sitetitle}}" href="{{link-mainpage}}" class="title-logo" style="-webkit-mask-image:url({{.}}); mask-image:url({{.}});"><span style="visibility:hidden;">Abc</span></a>
						{{/src}}
					</div>
				{{/wordmark}}
				{{^wordmark}}<a href="{{link-mainpage}}">{{msg-sitetitle}}</a>{{/wordmark}}
			{{/data-logos}}
		</h1>
		<div class="counters cell">
			<div class="counter articles"><big>{{html-articlecount}}</big> <span>{{msg-eve-pages}}</span></div>
			<div class="counter photos"><big>{{html-filecount}}</big> <span>{{msg-eve-files}}</span></div>
		</div>
		<div class="counters cell">
			<div class="counter edits"><big>{{html-editcount}}</big> <span>{{msg-eve-edits}}</span></div>
			<div class="counter users"><big>{{html-usercount}}</big> <span>{{msg-eve-users}}</span></div>
		</div>
		<div class="cpe-dropdown local-navigation">
			<a class="link cell cpe-dropdown__toggle" title="{{msg-eve-navigation-rest}}">
				<span class="cpe-icon cpe-icon-large cpe-dropdown__toggle-chevron material-icons">
					arrow_drop_down
				</span>
			</a>
			<div class="cpe-dropdown__content cpe-is-right-aligned cpe-is-not-scrollable">
				<ul class="cpe-list cpe-is-linked">
							{{#data-portlets-sidebar}}
							{{#data-portlets-first}}{{>PortletSLN2}}{{/data-portlets-first}}
							{{#array-portlets-rest}}{{>PortletSLN2}}{{/array-portlets-rest}}
							{{/data-portlets-sidebar}}
				</ul>
			</div>
		</div>
		<!-- Search -->
		<div class="cpe-dropdown search">
			<div class="link cell cpe-dropdown__toggle" title={{msg-search}}>
				<span class="cpe-icon material-icons">
					search
				</span>
			</div>
			<div class="cpe-dropdown__content">
				<ul class="cpe-list">
					<li>
					{{#data-search-box}}
					<form action="{{form-action}}" role="search" class="mw-portlet" id="p-search">
						<div class="pageheader-search">
							<input type="search" name="search" class="cpe-input cpe-is-borderline" placeholder="{{msg-searchsuggest-search}}" autocapitalize="sentences" accesskey="f" id="searchInput" autocomplete="off">
							<input style="width:0px; height:0px; visibility:hidden; position:absolute;" type="submit" id="searchButton2S"></input>
							<label for="searchButton2S" class="cpe-button cpe-is-borderline">
								<span class="cpe-icon cpe-icon-small material-icons">
									search
								</span>
							</label>
						</div>
					</form>
					{{/data-search-box}}
					</li>
				</ul>
			</div>
		</div>
		{{>CommunityLinks}}
	  <!-- Toggle -->
	  <a class="link nav-switch" title="{{msg-hide}}">
		<span class="cpe-icon cpe-dropdown__toggle-chevron material-icons cpe-icon-large">
			keyboard_arrow_up
		</span>
	  </a>
	</header>
	
	<header class="mpisto-sticky-header-container col-nav">
	  <a class="link nav-switch" title="{{msg-show}}">
		<span class="cpe-icon cpe-dropdown__toggle-chevron material-icons cpe-icon-large">
			keyboard_arrow_down
		</span>
	  </a>
	</header>