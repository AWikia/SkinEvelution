		<header class="evelution-page-header" id ="PageHeader">
		  <div class="evelution-page-header-main">
			<div class="evelution-page-header-title">
				  <a class="toggle-rail-button" title="{{msg-evelution-togglesize}}">
					<span class="cpe-icon material-icons rail-on">
						open_in_full
					</span>
					<span class="cpe-icon material-icons rail-off">
						close_fullscreen
					</span>
				  </a>
				{{>Notices}}
				<h1 class="evelution-title cpe-midlight-color" id="firstheading">{{html-title}}</h1>
			</div>
			<h4 class="evelution-page-header-subtitle" id="#contentSub">
				<span>{{msg-tagline}}</span>
				<span>{{{html-subtitle}}}</span>
				<span>{{{html-undelete-link}}}</span>			
			</h4>
				<div class="evelution-page-header-contribution">
					<div class="evelution-page-header-contribution-buttons">

						<div class="cpe-button-group color-modes color-management-on">
							<li class="auto auto-r">
								<button onclick="ColorMode()" class="cpe-button">
									<span class="cpe-icon cpe-icon-small material-icons margined">
										brightness_auto
									</span>
									<span>Duo</span>
								</button>
							</li>
							<li class="light dark">
								<button onclick="ColorMode('light','dark')" class="cpe-button">
									<span class="cpe-icon cpe-icon-small material-icons margined">
										<span class="systemdarkmode-off">
											light_mode
										</span>
										<span class="systemdarkmode-on">
											dark_mode
										</span>
									</span>
									<span>Solo</span>
								</button>
							</li>

						</div>

						<div class="cpe-button-group visual-appearance">
						  <div class="cpe-dropdown">				
							<div class="cpe-button cpe-dropdown__toggle">
								<span class="cpe-icon cpe-icon-small material-icons">
									invert_colors
								</span>
								<span>{{msg-evelution-color-filters}}</span>
								<span class="cpe-icon cpe-icon-tiny cpe-icon-large cpe-dropdown__toggle-chevron material-icons">
									arrow_drop_down
								</span>
							</div>
								<div class="cpe-dropdown__content is-left-aligned cpe-visual-appearance">					
								  <ul class="cpe-list is-linked cpe-color-filters">						
									<li id="f-standard"><a onclick="ColorFilter('standard')">{{msg-evelution-standard}}</a></li>
									<li id="f-invert"><a onclick="ColorFilter('invert')">{{msg-evelution-filter-invert}}</a></li>
									<li id="f-night"><a onclick="ColorFilter('night')">{{msg-evelution-filter-night}}</a></li>
									<li id="f-gray"><a onclick="ColorFilter('gray')">{{msg-evelution-filter-gray}}</a></li>
									<li id="f-darkgray"><a onclick="ColorFilter('darkgray')">{{msg-evelution-filter-darkgray}}</a></li>
									<li id="f-yelblk"><a onclick="ColorFilter('yelblk')">{{msg-evelution-filter-yelblk}}</a></li>
									<li id="f-bluwht"><a onclick="ColorFilter('bluwht')">{{msg-evelution-filter-bluwht}}</a></li>
								  </ul>
								</div>
							</div>
							<!-- Styles -->
							  <div class="cpe-dropdown">				
									<div class="cpe-button cpe-dropdown__toggle">
										<span class="cpe-icon cpe-icon-small material-icons">
											brush
										</span>
										<span>{{msg-evelution-visual-styles}}</span>
										<span class="cpe-icon cpe-icon-tiny cpe-icon-large cpe-dropdown__toggle-chevron material-icons">
											arrow_drop_down
										</span>
									</div>
									<div class="cpe-dropdown__content is-left-aligned cpe-visual-appearance">					
									  <ul class="cpe-list is-linked cpe-visual-styles">						
										<li id="styleS"><a onclick="VisualStyle('S')">{{msg-evelution-standard}}</a></li>
									  </ul>

									</div>
								</div>
							<!-- Colors -->
							  <div class="cpe-dropdown td-off color-management-on">				
									<div class="cpe-button cpe-dropdown__toggle">
										<span class="cpe-icon cpe-icon-small material-icons">
											palette
										</span>
										<span>{{msg-evelution-visual-colors}}</span>
										<span class="cpe-icon cpe-icon-tiny cpe-icon-large cpe-dropdown__toggle-chevron material-icons">
											arrow_drop_down
										</span>
									</div>
									<div class="cpe-dropdown__content is-right-aligned cpe-visual-appearance">					
									  <ul class="cpe-list is-linked cpe-visual-colors">						
										<li id="colorS"><a onclick="VisualColor('S')">{{msg-evelution-standard}}</a></li>
									  </ul>

									</div>
								</div>
							</div>

					  <div class="cpe-button-group views">
						{{#data-portlets.data-namespaces}}{{>PortletBTN}}{{/data-portlets.data-namespaces}}						
					  </div>
						<div class="cpe-button-group actions">
						{{#data-portlets.data-views}}{{>PortletBTN}}{{/data-portlets.data-views}}
						  <div class="cpe-dropdown more-actions-menu">				
							<div class="cpe-button cpe-dropdown__toggle">
								<span class="cpe-icon cpe-icon-tiny cpe-icon-large cpe-dropdown__toggle-chevron material-icons">
									more_vertical
								</span>
							</div>
								<div class="cpe-dropdown__content is-right-aligned">					
								<!--NameSpaces-->
									{{#data-portlets.data-actions}}{{>Portlet2}}{{/data-portlets.data-actions}}
								</div>
							</div>
						</div>
					</div>
				</div>
		  </div>
		 </header>
		 