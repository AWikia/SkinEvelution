{{>Icons}}
  <div class="gradient-overlay" style="position:absolute; pointer-events:none;"></div>
  <div class="body-community-background" style="position:absolute; pointer-events:none;"></div>
	<container>
		{{>GlobalNavigation}}
		<main>
		<aside class="mpisto-wikispace">
			<div class="top-gap"></div>
			{{>StickyHeader}}
			<section class="mpisto-content" id="mw-content" onclick="">
				{{>CommunityHeader}}
				<div class="mpisto-wrapper">
					<div class="mpisto-article-wrapper">
					  <nav class="mpisto-left-pane">
						{{>PageHeader}}
						<div class="mpisto-article-content">
							  {{>ServerActions}}
							  <!-- Begin Article -->
							  <article class="mpisto-article" id="content">
								{{>ContentBody}}
							  </article>
							  <!-- End Article -->
						  </div>
						  <!-- Begin Footer -->
						<div class="mpisto-page-header mpisto-page-footer">
							<div class="mpisto-page-header-main">
								<div class="mpisto-page-header-title">
									{{#data-footer}}
									{{!}}{{#data-info}}{{>FooterList}}{{/data-info}}
									{{/data-footer}}
								</div>
							</div>
						</div>
					  <!-- End Footer -->
					  </nav>
					  <nav class="mpisto-right-pane">
						{{>Modules}}
					  </nav>
					</div>
				</div>
			</section>
		{{>PersonalTools}}
		</aside>

		</main>
	</container>
