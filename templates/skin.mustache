	<div id="LunaLevit" style=" pointer-events: none; position: fixed; z-index: -2; width: 100%; height: 100%;">
		<div class="invertion-on" style=" pointer-events: none; position: fixed; z-index: -2; width: 100%; height: 100%;">
			<div id="LunaLevit3" class="desktop-background fandom-community-header__background" style="position:absolute; pointer-events:none;" id="desktop-background">
			</div>
		</div>
	</div>
	<container class="{{^has-server-mode}}home-mode{{/has-server-mode}}{{#has-server-mode}}server-mode{{/has-server-mode}}{{#has-disabled-rail}} no-rail{{/has-disabled-rail}}{{#has-forced-full-width}} forced-full{{/has-forced-full-width}}{{#has-no-color-management}} no-color-management{{/has-no-color-management}}" {{^has-servelution-vanadium}}sticky-rail="{{{html-sticky-rail}}}"{{/has-servelution-vanadium}} id="top"> <!--<container> -->
		<main class="main-container">
			<div class="top-gap" responsive></div>
			{{>CommunityHeader}}
			<section class="evelution-content resizable-container" responsive id="mw-content" onclick="">
				{{>TopActions}}
				<div class="evelution-wrapper page">
					{{>PageActions}}
					<div class="evelution-article-wrapper">
					  <div class="evelution-actions-with-article">
						  <nav class="evelution-article-container page__main">
							{{>PageHeader}}
							<div class="evelution-article-content">
								  {{>ServerActions}}
								  <!-- Begin Article -->
								  <article class="evelution-article page-content" id="content">
									{{>ContentBody}}
								  </article>
								  <!-- End Article -->
							  </div>
							  <!-- Begin Footer
							<div class="evelution-page-header evelution-page-footer page-header">
								<div class="evelution-page-header-main page-header__bottom">
									<div class="evelution-page-header-title page-header__title-wrapper">
										{{#data-footer}}
										{{!}}{{#data-info}}{{>FooterList}}{{/data-info}}
										{{/data-footer}}
									</div>
								</div>
							</div>
						  <!-- End Footer -->
						  </nav>
					  </div>
					  <nav class="evelution-rail-wrapper evelution-right-rail page__right-rail WikiaRail">
						{{>Modules}}
					  </nav>
					</div>
				</div>
			</section>
		{{>EvelutionFooter}}
		{{>Taskbar}}

		</main>
	</container>

