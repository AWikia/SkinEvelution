{{>Icons}}
	<div style="position:absolute; z-index:-1;">
		<span class="fc-buttonface" style="color:buttonface"></span>
		<span class="fc-buttontext" style="color:buttontext"></span>
		<span class="fc-canvas" style="color:canvas;"></span>
		<span class="fc-linktext" style="color:linktext;"></span>
		<span class="fc-graytext" style="color:graytext;"></span>
		<span class="fc-canvastext" style="color:canvastext;"></span>
		<span class="fc-highlight" style="color:highlight;"></span>
	</div>
	<div class="gradient-overlay" style="position:absolute; pointer-events:none;"></div>
	<div class="invertion-on" style=" pointer-events: none; position: absolute; z-index: -2; width: 100%; height: 100%;"><div class="body-background fandom-community-header__background" style="position:absolute; pointer-events:none;" id="body-background"></div></div>
	<div class="body-background-color" style="position:absolute; pointer-events:none;"></div>
	<container class="{{^has-server-mode}}home-mode{{/has-server-mode}}{{#has-server-mode}}server-mode{{/has-server-mode}}{{#has-disabled-rail}} no-rail{{/has-disabled-rail}}{{#has-single-header}} single-header{{/has-single-header}}{{#has-forced-full-width}} forced-full{{/has-forced-full-width}}{{#has-no-color-management}} no-color-management{{/has-no-color-management}}" sticky-rail="{{{html-sticky-rail}}}"> <!--<container> -->
		<main class="main-container">
			<div class="top-gap"></div>
			{{>StickyHeader}}
			<section class="evelution-content resizable-container" responsive id="mw-content" onclick="">
				{{>CommunityHeader}}
				<div class="evelution-wrapper page">
					<div class="evelution-article-wrapper">
					  <nav class="evelution-rail-wrapper evelution-left-rail page__right-rail WikiaRail">
						{{>Modules2}}
					  </nav>
					  <nav class="evelution-article-container page__main">
						{{>PageActions}}
						{{>PageHeader}}
						<div class="evelution-article-content">
							  {{>ServerActions}}
							  <!-- Begin Article -->
							  <article class="evelution-article page-content" id="content">
								{{>ContentBody}}
							  </article>
							  <!-- End Article -->
						  </div>
						  <!-- Begin Footer
						<div class="evelution-page-header evelution-page-footer page-header">
							<div class="evelution-page-header-main page-header__bottom">
								<div class="evelution-page-header-title page-header__title-wrapper">
									{{#data-footer}}
									{{!}}{{#data-info}}{{>FooterList}}{{/data-info}}
									{{/data-footer}}
								</div>
							</div>
						</div>
					  <!-- End Footer -->
					  </nav>
					  <nav class="evelution-rail-wrapper evelution-right-rail page__right-rail WikiaRail">
						{{>Modules}}
					  </nav>
					</div>
				</div>
			</section>
		{{>EvelutionFooter}}
		{{>Taskbar}}

		</main>
	</container>


<style>
	{{#has-no-color-management}}
		:root,
		:root:root {
			--desktop-background-image:url("")!important;
			--desktop-background-image-opacity:100%!important;
			--desktop-background-color:buttonface!important;
			--desktop-text-background-color:buttontext!important;
			--desktop-background-mode:standard!important;
			--desktop-background-horizontal-alignment:center!important;
			--desktop-background-vertical-alignment:center!important;
			--desktop-background-size:cover!important;
			--desktop-background-no-horizontal-tiling:false!important;
			--desktop-background-no-vertical-tiling:false!important;
			--hyperlink-background-color:linktext!important;
			--canvas-background-color:canvas!important;
			--inactive-text-background-color:graytext!important;
			--canvas-text-background-color:canvastext!important;
			--highlight-background-color:highlight!important;
			--active-title-background-color:highlight!important;
			--inactive-title-background-color:graytext!important;
			--custom-secondary-font:""!important;
			--border-radius:3px!important;
			--icon-filter:opacity(1)!important;
			--icon-filter-hover:opacity(1)!important;
			--icon-filter-duration:0ms!important;
			--icon-filter-delay:0ms!important;
			--system-acryllic-opacity:0.6!important;
		}
	{{/has-no-color-management}}
</style>
