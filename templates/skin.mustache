{{#has-servelution-unsupported}}
	<style>
		*, :before, :after {
			all:initial!important;
		}
		*:not(html):not(body):not(.warn) {
			display:none!important;
		}
		
	</style>
	<div class="warn">
		Evelution can't run on this Servelution client
		<br class="warn">
		Please upgrade your Servelution client to one that supports the CPE Language Recompiler
	</div>
{{/has-servelution-unsupported}}

