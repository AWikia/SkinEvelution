			"value": false,
			"description": "Moves upper taskbar links to the left. Has no effect on Landscape Taskbar"
		},
		"EvelutionDisableColorManagement": {
			"value": false,
			"description": "Disables all color-related settings, forcing system colors instead"
		},
		"EvelutionDisableRightRail": {
			"value": false,
			"description": "Disables Evelution's right rail completely"
		},
